<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/questionSetItemCardView"
    android:layout_marginTop="0dp"
    android:layout_marginBottom="15dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    app:cardBackgroundColor="@color/new_primary_variant"
    app:cardCornerRadius="@dimen/cardview_default_radius"
    >

    <TextView
        android:id="@+id/questionSetItemTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="10dp"
        android:paddingTop="6dp"
        android:textSize="18sp"
        android:textColor="@color/new_on_primary"
        android:textAlignment="center"
        android:text="@string/category_name_dummy"
        />

</androidx.cardview.widget.CardView>